AAA,MAAA,EAAA;AAAA,QAAA,IAAA,EAAA,IAAA;AAAA,QAAA,KAAA,EAAA;AAAA,OAAA;AACA;AAHA;AApDA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <slot name=\"header\"></slot>\r\n    <div>\r\n      <swiper ref=\"mySwiper\" :options=\"swiperOptions\">\r\n        <swiper-slide v-for=\"(item, idx) in listData\" :key=\"idx\" @click=\"jump(item,idx)\">\r\n          {{ item.text }}\r\n          <img :src=\"item.imgPath\" alt=\"\" />\r\n        </swiper-slide>\r\n        <div class=\"swiper-pagination\" slot=\"pagination\"></div>\r\n      </swiper>\r\n      <slot name=\"footer\"></slot>\r\n    </div>\r\n    <navList></navList>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { Swiper, SwiperSlide, directive } from \"vue-awesome-swiper\";\r\n\r\n// import style (>= Swiper 6.x)\r\nimport \"swiper/swiper-bundle.css\";\r\n\r\n// // import style (<= Swiper 5.x)\r\n// import \"swiper/css/swiper.css\";\r\n\r\nexport default {\r\n  name: \"home\",\r\n  props: {\r\n    listData: {\r\n      type: Array,\r\n      default() {\r\n        return [\r\n          {\r\n            id: \"\",\r\n            text: \"图片1\",\r\n            imgPath: \"https://dpic.tiankong.com/1j/85/QJ6100354990.jpg@!350h\",\r\n          },\r\n          {\r\n            id: \"\",\r\n            text: \"图片2\",\r\n            imgPath: \"https://dpic.tiankong.com/1j/85/QJ6100354990.jpg@!350h\",\r\n          },\r\n          {\r\n            id: \"\",\r\n            text: \"图片3\",\r\n            imgPath: \"https://dpic.tiankong.com/1j/85/QJ6100354990.jpg@!350h\",\r\n          },\r\n        ];\r\n      },\r\n    },\r\n  },\r\n  data() {\r\n    return {\r\n      swiperOptions: {\r\n        pagination: {\r\n          el: \".swiper-pagination\",\r\n        },\r\n        // Some Swiper option/callback...\r\n      },\r\n    };\r\n  },\r\n  components: {\r\n    Swiper,\r\n    SwiperSlide,\r\n  },\r\n  computed: {\r\n    swiper() {\r\n      return this.$refs.mySwiper.$swiper;\r\n    },\r\n  },\r\n  directives: {\r\n    swiper: directive,\r\n  },\r\n  mounted() {\r\n    console.log(\"Current Swiper instance object\", this.swiper);\r\n    this.swiper.slideTo(3, 1000, false);\r\n  },\r\n  methods: {\r\n      jump(data,index) {\r\n          this.$emit('jump',{data:data,index:index})\r\n      },\r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>\r\n"],"sourceRoot":"src/components/swiper"}]}